<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.my.mobilesafe.activity.SettingActivity">

    <TextView
        style="@style/TitleStyle"
        android:text="设置中心"/>
    <com.my.mobilesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.my.mobilesafe"
        android:id="@+id/siv_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="自动更新设置"
        mobilesafe:desoff="自动更新已关闭"
        mobilesafe:deson="自动更新已开启">
    </com.my.mobilesafe.view.SettingItemView>

    <com.my.mobilesafe.view.SettingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.my.mobilesafe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="电话归属地的设置"
        mobilesafe:desoff="归属地设置已关闭"
        mobilesafe:deson="归属地设置已开启">
    </com.my.mobilesafe.view.SettingItemView>

</LinearLayout>
